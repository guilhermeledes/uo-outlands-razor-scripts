@setvar! skillcap 100

if skill evalint < skillcap
    skill evalint
    wft 1000
    hotkey "target random friendly"
    wait 2500
    gumpclose
else
  overhead 'Congratulations you have finished your training time!'
  wait 5000
endif

loop